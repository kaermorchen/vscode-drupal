{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Server",
      "type": "node",
      "request": "attach",
      "port": 6009,
      "restart": true,
      "outFiles": [
        "${workspaceRoot}/out/**/*.js"
      ]
    },
    {
      "name": "Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "outFiles": [
        "${workspaceFolder}/out/extension.js",
      ],
      "preLaunchTask": "npm: watch"
    },
  ],
  "compounds": [
    {
      "name": "Extension + Server",
      "configurations": [
        "Extension",
        "Server"
      ]
    }
  ]
}
